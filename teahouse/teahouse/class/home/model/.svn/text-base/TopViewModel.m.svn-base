#import "TopViewModel.h"

@implementation TopViewModel

+ (instancetype)modelWithTitle:(NSString *)title image:(NSString *)btnImage color:(NSString *)titleColor {
    return [[self alloc] initWithTitle:title image:btnImage color:titleColor];
}

- (instancetype)initWithTitle:(NSString *)title image:(NSString *)btnImage color:(NSString *)titleColor {
    if (self = [super init]) {
        self.btnTitle = title;
        self.btnImage = btnImage;
        self.titleColor = titleColor;
    }
    return self;
}
@end
